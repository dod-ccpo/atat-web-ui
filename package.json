{
  "name": "atat-web-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --https",
    "build": "vue-cli-service build",
    "test:unit-watch": "vue-cli-service test:unit --watch ",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "serve-dev": "vue-cli-service serve --mode development --https",
    "test": "npm run lint && npm run test:unit && npm run test:e2e",
    "test:browserstack:firefox": "vue-cli-service test:browserstack -c tests/e2e/browserstack_config/nightwatch.conf.js -e firefox tests/e2e/specs/test.js",
    "test:browserstack:ie11": "vue-cli-service test:browserstack -c tests/e2e/browserstack_config/nightwatch.conf.js -e ie11 tests/e2e/specs/test.js",
    "test:browserstack": "vue-cli-service test:browserstack -c tests/e2e/browserstack_config/nightwatch.conf.js -e chrome tests/e2e/specs/test.js",
    "test:e2e:local": "vue-cli-service test:e2e --config tests/e2e/nightwatch.json -e ie,edge,chrome -t tests/e2e/specs/sampletest.js",
    "test:ie:local": "vue-cli-service test:e2e --config tests/nightwatch2.js -e selenium.ie -t tests/e2e/specs/sampletest.js",
    "start-seleniumserver": "java -jar node_modules\\selenium-server\\lib\\runner\\selenium-server-standalone-3.141.59.jar",
    "test:ie": "npm-run-all --parallel start-seleniumserver test:ie:local",
    "test:chrome:local": "vue-cli-service test:e2e --config tests/nightwatch2.js -e webdriver.chrome -t tests/e2e/specs/sampletest.js",
    "start-chrome-driver": "chromedriver",
    "start-edge-driver": "MicrosoftWebDriver.exe --port=17556",
    "test:edge:local": "vue-cli-service test:e2e --config tests/nightwatch2.js -e webdriver.edge -t tests/e2e/specs/sampletest.js",
    "test:e2e:local:webdriver": "vue-cli-service test:e2e --config tests/nightwatch2.js -e webdriver.edge,webdriver.chrome -t tests/e2e/specs/sampletest.js",
    "test:e2e:run-local-webdriver": "npm-run-all --parallel start-chrome-driver start-edge-driver test:e2e:local:webdriver -r",
    "test:e2e:wd:local": "npm-run-all --parallel test:e2e:run-local-webdriver test:ie"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "css-vars-ponyfill": "^2.4.5",
    "eslint-plugin-wdio": "^7.4.2",
    "material-design-icons": "^3.0.1",
    "register-service-worker": "^1.7.1",
    "sinon": "^11.1.2",
    "typeface-source-sans-pro": "^1.1.13",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-moment": "^4.1.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@types/jest": "^24.0.19",
    "@types/mocha": "^8.0.1",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-nightwatch": "^4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.0.3",
    "browserstack-automate": "^1.0.2",
    "chromedriver": "^91.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "geckodriver": "^1.22.3",
    "iedriver": "^3.150.1",
    "nightwatch": "^1.3.4",
    "selenium-server": "^3.141.59",
    "prettier": "^2.2.1",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "ts-node": "^8.10.2",
    "typescript": "~4.1.5",
    "vue-cli-plugin-e2e-nightwatch-browserstack": "^4.0.1",
    "vue-cli-plugin-vuetify": "~2.4.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules"
    ]
  },
  "engines": {
    "node": "^14.16.0"
  }
}