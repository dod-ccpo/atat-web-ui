{
  "name": "atat-web-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve-dev": "vue-cli-service serve --mode development --https",
    "serve-dev-win": "vue-cli-service serve --mode development --host localhost --watch",
    "serve": "vue-cli-service serve --https",
    "build": "vue-cli-service build",
    "test:unit-watch": "vue-cli-service test:unit --watch ",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.js' --collectCoverageFrom='src/**/*.vue'",
    "test-file:watch": "jest --watch --no-cache src/wizard/Step1/views/Step1.spec.ts ",
    "test:browserstack:edge": "vue-cli-service test:browserstack -c tests/e2e/browserstack_config/nightwatch.conf.js -e edge dist/tests/e2e/page_tests/test.js",
    "test:browserstack:ie11": "vue-cli-service test:browserstack -c tests/e2e/browserstack_config/nightwatch.conf.js -e ie11 dist/tests/e2e/page_tests/test.js",
    "test:browserstack:chrome": "vue-cli-service test:browserstack -c tests/e2e/browserstack_config/nightwatch.conf.js -e chrome dist/tests/e2e/page_tests/test.js",
    "start-chrome-driver": "chromedriver",
    "start-edge-driver": "MicrosoftWebDriver.exe --port=17556",
    "test:ie": "tsc && vue-cli-service test:e2e --config tests/nightwatch.conf.js -e selenium.ie -t dist/tests/e2e/page_tests/",
    "test:chrome:local": "vue-cli-service test:e2e --config tests/nightwatch.conf.js -e webdriver.chrome -t dist/tests/e2e/page_tests/",
    "test:chrome": "tsc && npm-run-all --parallel start-chrome-driver test:chrome:local -r",
    "test:edge:local": "vue-cli-service test:e2e --config tests/nightwatch.conf.js -e webdriver.edge -t dist/tests/e2e/page_tests/",
    "test:edge": "tsc && npm-run-all --parallel  start-edge-driver test:edge:local -r",
    "test:nightwatch": "vue-cli-service test:e2e --config tests/nightwatch.conf.js -e webdriver.chrome,webdriver.edge,selenium.ie -t dist/tests/e2e/page_tests/",
    "test:ts": "tsc && npm-run-all --parallel start-chrome-driver start-edge-driver test:nightwatch -r"
  },
  "dependencies": {
    "@wdio/cli": "^7.7.4",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "css-vars-ponyfill": "^2.4.5",
    "eslint-plugin-wdio": "^7.4.2",
    "material-design-icons": "^3.0.1",
    "register-service-worker": "^1.7.1",
    "typeface-source-sans-pro": "^1.1.13",
    "vue": "^2.6.11",
    "vue-axios": "^3.2.5",
    "vue-class-component": "^7.2.3",
    "vue-moment": "^4.1.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "@types/chromedriver": "^81.0.0",
    "@types/jest": "^24.0.19",
    "@types/mocha": "^8.0.1",
    "@types/nightwatch": "^1.3.4",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-nightwatch": "^4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.0.3",
    "browserstack-automate": "^1.0.2",
    "chromedriver": "^93.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-wdio": "^7.4.2",
    "iedriver": "^3.150.1",
    "material-design-icons-iconfont": "^6.1.0",
    "nightwatch": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "node-sass": "^6.0.1",
    "prettier": "^2.2.1",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "selenium-server": "^3.141.59",
    "ts-node": "^8.10.2",
    "typescript": "~4.1.5",
    "vue-cli-plugin-e2e-nightwatch-browserstack": "^4.0.1",
    "vue-cli-plugin-vuetify": "~2.4.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^1.0.1",
    "wdio-chromedriver-service": "^6.0.3"
  },
  "engines": {
    "node": "^14.16.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules"
    ]
  }
}
